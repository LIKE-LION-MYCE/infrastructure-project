{
  "dashboard": {
    "id": null,
    "uid": "spring-boot-myce",
    "title": "MYCE Spring Boot Application",
    "tags": ["spring-boot", "myce", "java"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "type": "stat",
        "title": "Application Status",
        "targets": [
          {
            "expr": "up{job=\"spring-boot-app\"}",
            "legendFormat": "Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "UP"},
              {"type": "value", "value": "0", "text": "DOWN"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "type": "stat",
        "title": "JVM Memory Usage",
        "targets": [
          {
            "expr": "(jvm_memory_used_bytes{area=\"heap\"} / jvm_memory_max_bytes{area=\"heap\"}) * 100",
            "legendFormat": "Heap Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "type": "stat",
        "title": "Active DB Connections",
        "targets": [
          {
            "expr": "hikaricp_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 3},
                {"color": "red", "value": 4}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "type": "stat",
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count[1m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 4},
        "type": "graph",
        "title": "HTTP Request Rate by Endpoint",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count[5m])",
            "legendFormat": "{{method}} {{uri}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 4},
        "type": "graph",
        "title": "Response Time Percentiles",
        "targets": [
          {
            "expr": "http_server_requests_seconds{quantile=\"0.5\"}",
            "legendFormat": "p50"
          },
          {
            "expr": "http_server_requests_seconds{quantile=\"0.95\"}",
            "legendFormat": "p95"
          },
          {
            "expr": "http_server_requests_seconds{quantile=\"0.99\"}",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 10},
        "type": "graph",
        "title": "JVM Heap Memory",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\"}",
            "legendFormat": "Used"
          },
          {
            "expr": "jvm_memory_committed_bytes{area=\"heap\"}",
            "legendFormat": "Committed"
          },
          {
            "expr": "jvm_memory_max_bytes{area=\"heap\"}",
            "legendFormat": "Max"
          }
        ],
        "yaxes": [
          {"format": "bytes", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 10},
        "type": "graph",
        "title": "Database Connection Pool",
        "targets": [
          {
            "expr": "hikaricp_connections_active",
            "legendFormat": "Active"
          },
          {
            "expr": "hikaricp_connections_idle",
            "legendFormat": "Idle"
          },
          {
            "expr": "hikaricp_connections_pending",
            "legendFormat": "Pending"
          },
          {
            "expr": "hikaricp_connections_max",
            "legendFormat": "Max"
          }
        ],
        "yaxes": [
          {"format": "short", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 10},
        "type": "graph",
        "title": "Garbage Collection",
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_sum[5m])",
            "legendFormat": "GC Pause Time"
          },
          {
            "expr": "rate(jvm_gc_pause_seconds_count[5m])",
            "legendFormat": "GC Count"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"format": "short", "show": true}
        ]
      },
      {
        "id": 10,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "WebSocket Connections (Chat)",
        "targets": [
          {
            "expr": "stomp_websocket_connected_sessions",
            "legendFormat": "Connected Sessions"
          }
        ],
        "yaxes": [
          {"format": "short", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 11,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 16},
        "type": "table",
        "title": "Top 10 Slowest Endpoints",
        "targets": [
          {
            "expr": "topk(10, http_server_requests_seconds{quantile=\"0.99\"})",
            "format": "table",
            "instant": true
          }
        ]
      },
      {
        "id": 12,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 22},
        "type": "stat",
        "title": "Error Rate (5xx)",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 22},
        "type": "stat",
        "title": "MongoDB Operations",
        "targets": [
          {
            "expr": "rate(spring_data_mongodb_operations_total[5m])",
            "legendFormat": "Operations/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 14,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 22},
        "type": "stat",
        "title": "Redis Commands",
        "targets": [
          {
            "expr": "rate(spring_data_redis_operations_total[5m])",
            "legendFormat": "Commands/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 22},
        "type": "stat",
        "title": "Thread Count",
        "targets": [
          {
            "expr": "jvm_threads_live_threads",
            "legendFormat": "Live Threads"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 200}
              ]
            }
          }
        }
      }
    ]
  }
}