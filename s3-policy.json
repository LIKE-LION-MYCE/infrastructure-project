{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3MediaBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:PutObjectAcl",
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::myce-media-bucket/*"
      ]
    },
    {
      "Sid": "S3MediaBucketList",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::myce-media-bucket"
      ]
    }
  ]
}